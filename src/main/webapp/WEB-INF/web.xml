<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<display-name>SaveNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SaveNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteNoteServlet</servlet-name>
  	<display-name>DeleteNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.DeleteNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>EditNoteServlet</servlet-name>
  	<display-name>EditNoteServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.EditNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TitleSearchServlet</servlet-name>
  	<display-name>TitleSearchServlet</display-name>
  	<description></description>
  	<servlet-class>com.servlet.TitleSearchServlet</servlet-class>
  </servlet>
  
  
  
  <servlet-mapping>
  	<servlet-name>SaveNoteServlet</servlet-name>
  	<url-pattern>/SaveNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteNoteServlet</servlet-name>
  	<url-pattern>/DeleteNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>EditNoteServlet</servlet-name>
  	<url-pattern>/EditNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TitleSearchServlet</servlet-name>
  	<url-pattern>/TitleSearchServlet</url-pattern>
  </servlet-mapping>
  
  
</web-app>

